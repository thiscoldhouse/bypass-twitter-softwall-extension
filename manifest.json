{
  "description": "Show me twitter. I do not want to login.",
  "manifest_version": 2,
  "name": "bypass-twitter-softwall",
  "version": "1.4",
  "content_scripts": [
    {
      "matches": [
        "*://twitter.com/*",
        "*://mobie.twitter.com/*",
        "*://twitter3e4tixl4xyajtrzo62zg5vztmjuricljdp2c5kshju4avyoid.onion/*"
      ],
      "js": ["main.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_title": "Bypass Twitter Login",
    "default_icon": {
      "48": "logo48.png"
    }
  }
}
